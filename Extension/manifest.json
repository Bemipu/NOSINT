{
    "name": "NOSINT - OSINT safeguard",
    "description": "A friendly reminder prevent you leaking data",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": "leak.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"]
        }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["tabs", "storage"]
}